<Window x:Class="CookieClickerClicker.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CookieClickerClicker"
        mc:Ignorable="d"
        Title="CookieClickerManager" Height="400" Width="1200">
    <Canvas x:Name="AppContainer" Loaded="WebContainer_OnLoaded">
        <Canvas x:Name="DebugWrapper">
            <Button x:Name="PauseBtn" PreviewMouseUp="PauseBtn_OnPreviewMouseUp" Content="Pause"/>
            <ListBox x:Name="DebugBox"/>
        </Canvas>
        <Canvas x:Name="ProductTierWrapper">
            <ListBox x:Name="ProductTierBox"/>
        </Canvas>
        <Canvas x:Name="BrowserWrapper">
        </Canvas>
    </Canvas>
</Window>
